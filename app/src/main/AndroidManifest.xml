<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidnotetakingproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DocumentListView" />
        <activity android:name=".createNewAccount" />
        <activity android:name=".createAccountPartTwo" />
        <activity android:name=".forgotPassword" />
        <activity android:name=".notepad" />
        <activity android:name=".UserSetting" />
        <activity android:name=".shareDocument" />
        <activity android:name=".chooseDocuments" />

    </application>

</manifest>